.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

#footer,#header,footer,header {
    /*position: absolute;*/
    z-index: 2;
    left: 0;
    right: 0;
    padding: 0;
    width: 100%;
    background-color: #fff;
    height:70rpx;
}

.head-center {
    vertical-align: middle;
    height: 44px;
    line-height: 44px;
    font-size: 14px
}
.head-center em,.head-center span {
    font-size: 30rpx
}
.head-center,.side-menu li {
    width: 100%;
    text-align: center
}
.search {
  position:absolute;
  right:25rpx;
  top:8rpx;
}
.swiper-item{
    display: block;
    height: 100rpx;
}
.swiper-view{
  position:relative;
  top:0rpx;
  height:100%;
}
.section_gap{
    position:relative;
    height:140rpx;
    /*background: url(../../image/moviebg.jpg) no-repeat 0 0 / 100% auto;*/
    top:-8rpx;
}
.section_gap .title,
.section_gap .time{
    text-align:center;
}
.section_gap .title{
    padding:10rpx 0 5rpx;
}
.section_gap .title text{
    font-size:28rpx;
}
.section_gap .time text{
    font-size:24rpx;
}
.section_gap .otherinfo{
    position:absolute;
    top:15rpx;
    right:20rpx;
    text-align:center;
}
.section_gap .otherinfo .infotxt {
    font-size:22rpx;
    font-weight:normal;
}
.section_gap .otherinfo .number text{
    font-size:40rpx;
}
.moviebg{
    width:100%;
    /*height:1.866667rem;*/
    /*position:relative;
    top:70rpx;*/
    height:130rpx;
    position:absolute;
}
