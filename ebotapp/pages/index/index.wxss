}

.screen-icon{
    width: 40rpx;
    height: 40rpx;
    position:absolute;
    right: 20rpx;
    top:145rpx;
}
.jt-core:after {
    position:absolute;
    content:"";
    top:3px;
    border-right:1px solid #aaa;
    border-bottom:1px solid #aaa;
    -webkit-transform:rotate(43deg);
    transform:rotate(43deg);
    width:6px;
    height:6px;
    left:136rpx;
}
.jt-core1::before {
    position:absolute;
    content:"";
    top:10px;
    border-right:1px solid #aaa;
    border-bottom:1px solid #aaa;
    -webkit-transform:rotate(43deg);
    transform:rotate(130deg);
    width:6px;
    height:6px;
    /*left:136rpx;*/
}

.jt-core2::before {
    position:absolute;
    content:"";
    top:10px;
    border-right:1px solid #aaa;
    border-bottom:1px solid #aaa;
    -webkit-transform:rotate(43deg);
    transform:rotate(315deg);
    width:6px;
    height:6px;
    right:10rpx;
}
.btn-pre{
    position: absolute;
    left:20rpx;
}
.block-data {
    text-align: center;
    padding-top: .16rem;
    padding-bottom: .133333rem;
    box-sizing: border-box;
    tap-highlight-color: transparent;
    -webkit-tap-highlight-color: transparent;
    -ms-tap-highlight-color: transparent;
}
